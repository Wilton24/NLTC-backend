    "serve": "nodemon --watch src --exec ts-node src/index.ts",
    "migration:create": "npx sequelize-cli migration:create --name",
    "migrate": "env-cmd npx sequelize-cli db:migrate",
    "migration:undo": "env-cmd npx sequelize-cli db:migrate:undo",
    "seeder:create": "npx sequelize seed:generate --name",
    "seeder:undo": "env-cmd npx sequelize-cli db:seed:undo",
    "seed": "env-cmd npx sequelize-cli db:seed:all"
  },
